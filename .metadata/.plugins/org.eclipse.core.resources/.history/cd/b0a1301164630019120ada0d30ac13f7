package StringAssign;

import java.util.Comparator;

public class CompareClass implements Comparable<String>{
	
	private String name;
	
	public CompareClass (String a)
	{
	
}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int compareTo(CompareClass o) {
		
		if(this.getName()== o.getName())
		 return 0;
		else if(this.getName().length()> o.getName().length())
			return 1;
		else if(this.getName().length()<o.getName().length())
			return -1;
		return 0;		
		
	}

	@Override
	public int compareTo(String o) {
		// TODO Auto-generated method stub
		return 0;
	}
}


	

		