package com.dac.GenericDemo;

	
	public class CustomMap<K,V> {
		K key;
		V value;        // K,V can be genric type int string char float
		
		public void putEntry(K key, V value){
			this.key = key;
			this.value = value;
		}
		
		public V get(K key){
			return value;
		}
		
		public K getKey(){
			return key;
		}
		
		public V getValue(){
			return value;
		}
	}


